<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>Email</title>
<!-- The styles -->
<link id="bs-css" href="css/bootstrap-cerulean.css" rel="stylesheet">
<style type="text/css">
body {
	padding-bottom: 40px;
}
.sidebar-nav {
	padding: 9px 0;
}
</style>
<link href="css/bootstrap-responsive.css" rel="stylesheet">
<link href='css/uniform.default.css' rel='stylesheet'>
<link href='css/colorbox.css' rel='stylesheet'>
<link href='css/jquery.cleditor.css' rel='stylesheet'>
<link href='css/opa-icons.css' rel='stylesheet'>
<!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
<!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

<!-- The fav icon -->
<!--<link rel="shortcut icon" href="img/favicon.ico">-->
<!--<script type="text/javascript" src="js/jquery.js"></script>-->
</head>
<body>

	<div class="row-fluid sortable">
		<div class="box-content">
			<table
				class="table table-striped table-bordered bootstrap-datatable datatable">
				<thead>
					<tr>
						<th>MessageName</th>
						<th>repository_name</th>
						<th>Sender</th>
						<th>recipients</th>
						<th>last_updated</th>
						<th>Operating</th>
					</tr>
				</thead>
				<tbody id="dataTable">
		<% for(var i=0; i<inboxs.length; i++) { %>      
        <tr class="success">
		     <td  class="center" ><%= inboxs[i]['message_name'] %></td>
			 <td  class="center"><%=inboxs[i]['repository_name']%></td>
			 <td  class="center"><%=inboxs[i]['sender']%></td>
			 <td class="center"><%=inboxs[i]['recipients']%></td>
			 <td class="center"><%=inboxs[i]['last_updated']%></td>
			 <td>
			 <a class="btn btn-danger" href="userListService?format=delete&name">
			 <i class="icon-trash icon-white"></i>Delete</a>
			 </td>
		</tr>
    <% } %>
				</tbody>
			</table>

		</div>
	</div>
</body>
</html>